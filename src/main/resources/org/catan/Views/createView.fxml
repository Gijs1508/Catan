                <Font name="Myanmar Text" size="29.0" />
            </font>
        </Button>
      <Text fx:id="game_code_text" layoutX="397.0" layoutY="485.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Code" textAlignment="CENTER" wrappingWidth="146.45751953125">
