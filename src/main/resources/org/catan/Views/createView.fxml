      <Button fx:id="join_game_btn" alignment="CENTER" contentDisplay="CENTER" layoutX="806.0" layoutY="472.0" mnemonicParsing="false" onAction="#joinGame" prefHeight="49.0" prefWidth="140.0" styleClass="btn" stylesheets="@../assets/style/style.css" text="Join game">
         <font>
            <Font name="Myanmar Text" size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
