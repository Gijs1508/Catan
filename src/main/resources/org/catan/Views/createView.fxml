            <Font size="33.0" />
         </font>
      </Text>
      <Button fx:id="back_to_menu_btn" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="463.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="49.0" prefWidth="140.0" styleClass="btn" stylesheets="@../assets/style/style.css" text="Terug naar menu">
