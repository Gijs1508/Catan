                <Image url="@../assets/img/Catan_glow.png" />
            </image>
        </ImageView>
        <Button fx:id="create_game_btn" alignment="CENTER" contentDisplay="CENTER" layoutX="366.0" layoutY="331.0" mnemonicParsing="false" onAction="#createGame" prefHeight="91.0" prefWidth="209.0" styleClass="btn" stylesheets="@../assets/style/style.css" text="Create game">
